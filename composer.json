{
    "name": "park-manager/hub-kit",
    "description": "HubKit allows project(s) maintainers to easily manage there GitHub repositories",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastiaan Stok",
            "email": "s.stok@rollerscapes.net"
        }
    ],
    "require": {
        "php": "^7.1",
        "symfony/process": "~3.3.6",
        "symfony/event-dispatcher": "~3.3.6",
        "symfony/filesystem": "~3.3.6",
        "symfony/console": "^3.2@dev",
        "webmozart/console": "dev-master@dev",
        "pimple/pimple": "^3.0",
        "guzzlehttp/psr7": "^1.2",
        "php-http/guzzle6-adapter": "^1.0",
        "knplabs/github-api": "^2.0.0",
        "ddd/slug": "~1.0.0",
        "rollerworks/version": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6",
        "symfony/phpunit-bridge": "^3.2.3",
        "symfony/debug": "^3.2.3"
    },
    "autoload": {
        "psr-4": {
            "HubKit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HubKit\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
